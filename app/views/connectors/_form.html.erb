<%= form_for(@connector) do |f| %>
  <% if @connector.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@connector.errors.count, "error") %> prohibited this connector from being saved:</h2>

      <ul>
      <% @connector.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :channel %><br>
    <%= f.text_field :channel %>
    Need to retreieve all the channels mugbot is in with this: <%= Mugbot %>
  </div>
  <div class="field">
    <%= f.label :mug_number %><br>
    <%= f.text_field :mug_number %>
    <select>
    <% TwilioClient.incoming_phone_numbers.list().each do |number| %>
      <option val="<%= number.phone_number %>"><%= number.phone_number %></option>
    <% end %>
    </select>
  </div>
  <div class="field">
    <%= f.label :user_number %><br>
    <%= f.text_field :user_number %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
